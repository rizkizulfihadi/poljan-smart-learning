{
    "version": 2,
    "builds": [
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/dist/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "server/server.js" },
      { "src": "/(.*)", "dest": "frontend/dist/$1" }
    ]
  }
  